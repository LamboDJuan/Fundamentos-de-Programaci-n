using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Desafio_01
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine("Ingrese el angulo b y los lados z, y en ese orden...");
            double a = double.Parse(Console.ReadLine());
            double b = double.Parse(Console.ReadLine());
            double c = double.Parse(Console.ReadLine());
          

            //Lados del triangulo pequeno
            double z = c * Math.Sin(c);
            double y = c * Math.Cos(c);

            //Angulo d
            double d = Math.Asin(z / b); 

            //Lado x
            double x = ((Math.Cos(d) * b) - y);

            Console.WriteLine("El valor del lado x es:" + x);




        }
    }
}
