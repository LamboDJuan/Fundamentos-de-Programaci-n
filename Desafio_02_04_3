using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Desafio_02_04_3
{
    class Program
    {
        static void Main()
        {
            Console.Write("Ingrese w: ");
            double w = double.Parse(Console.ReadLine());
            Console.Write("Ingrese d en grados: ");
            double dg = double.Parse(Console.ReadLine());
            Console.Write("Ingrese x: ");
            double x = double.Parse(Console.ReadLine());

            double t = (Math.Pow(2, w)) + (Math.Pow(2, x)) - ((2*w*x) - (Math.Cos(dg)));
            double tf = (Math.Sqrt(t));
            
            

        }
    }
}
